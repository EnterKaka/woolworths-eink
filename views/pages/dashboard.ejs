        <script>
            $(document).ready(function(){
                $("[id^=btn-]").hide();
                $('div.container_main').addClass('small');
                $('a[data-action="expand"]').click(() => {
                    $('div.container_main').toggleClass('small');
                    $('.dashborad_three #viewer_3d').toggleClass('expand');
                });
                setInterval(() => {
                    $.post('/data/getloadflag',function(req){
                        console.log(req);
                        if(req == 'success')
                        location.href = '/dashboard';
                    });
                }, 10000);
                $("#datepicker_from").datepicker({ 
                        autoclose: true, 
                        todayHighlight: true
                }).datepicker('update', new Date());
                $("#datepicker_to").datepicker({ 
                        autoclose: true, 
                        todayHighlight: true
                }).datepicker('update', new Date());
            });
            $('.delete_group').click(function(){
                $(this).children('.delete_modal').toggleClass('delete_modal_show');
            });
            $('.view_panel').on('click','.view_close',function(){
                $(this).parent().hide();
            });
            /*
            //////////////////////////////////////////////////////////////////////////////////
            delete data and model.
            //////////////////////////////////////////////////////////////////////////////////
            */

            $('.delete_single').click(function(){
                var id = $(this).attr('id');
                var buffer = id.split("delete_single-");
                var model_name = buffer[1];
                var obj_id = document.getElementById("delete_singleid-" + model_name).value;
                if(obj_id == ''){
                    // $.toast({
                    //     heading: 'Info',
                    //     text: 'Select Model',
                    //     showHideTransition: 'slide',
                    //     icon: 'info',
                    //     hideAfter: 5000   // in milli seconds
                    // });
                    return;
                }
                    

                const swalWithBootstrapButtons = Swal.mixin({
                customClass: {
                    confirmButton: 'btn btn-success',
                    cancelButton: 'btn btn-danger'
                },
                buttonsStyling: false
                })

                swalWithBootstrapButtons.fire({
                title: 'Are you sure?',
                text: "You won't be able to revert this!",
                icon: 'warning',
                showCancelButton: true,
                confirmButtonText: 'Yes, delete it!',
                cancelButtonText: 'No, cancel!',
                reverseButtons: true
                }).then((result) => {
                if (result.isConfirmed) {
                    $.post('/data/delete', { _id: obj_id }, function (data) {
                        if(data === 'success') {
                            swalWithBootstrapButtons.fire(
                            'Deleted!',
                            'Your file has been deleted.',
                            'success'
                            );
                            document.location.reload();
                        }
                    });

                    // url = '/user/delete/' + url_del;
                    // $.get(url, function( data ) {
                    // });
                }
                })
            });
            $('.delete_model').click(function(){
                var id = $(this).attr('id');
                var buffer = id.split("delete_model-");
                var model_name = buffer[1];
                console.log(model_name)
                const swalWithBootstrapButtons = Swal.mixin({
                customClass: {
                    confirmButton: 'btn btn-success',
                    cancelButton: 'btn btn-danger'
                },
                buttonsStyling: false
                })

                swalWithBootstrapButtons.fire({
                title: 'Are you sure?',
                text: "You won't be able to revert this!",
                icon: 'warning',
                showCancelButton: true,
                confirmButtonText: 'Yes, delete it!',
                cancelButtonText: 'No, cancel!',
                reverseButtons: true
                }).then((result) => {
                if (result.isConfirmed) {
                    $.post('/data/delete_model', { model_name: model_name }, function (data) {
                        if(data === 'success') {
                            swalWithBootstrapButtons.fire(
                            'Deleted!',
                            'Your file has been deleted.',
                            'success'
                            );
                            document.location.reload();
                        }
                    });

                    // url = '/user/delete/' + url_del;
                    // $.get(url, function( data ) {
                    // });
                }
                })
            });
            /*
            //////////////////////////////////////////////////////////////////////////////////
            material name by period
            //////////////////////////////////////////////////////////////////////////////////
            */
            $('.material_name').click(function(){
				$('#material_modal').modal('show');
                let name = $(this).attr('id').split("material_name-")[1];
                $('#material_model_title').html(name);
                let string = $('#actual_material_name-' + name).children('input').val();
                if(string === '') {
                    $('#material_name').val('');
                }else{
                    string = JSON.parse(string);
                    $('#material_name').val(string.material_name);
                }
            });
            $('#material_save').click(function(){
                param = {
                    name: $('#material_model_title').html(),
                    material_name: $('#material_name').val(),
                    from: $('#material_from').val(),
                    to: $('#material_to').val(),
                };
				$.post( '/dashboard/set_material_name', param, function( result ) {
                    if(result === 'success'){
						// updateGraph(param.name,'start');
						// showMessage('success','SCHEDULE', "Saved Successfully.");
						$('#material_modal').modal('hide');
					}
					else{
						// showMessage('error', 'SCHEDULE', "Failed");
					}
				});
            });
            /*
            //////////////////////////////////////////////////////////////////////////////////
            set actual material name
            //////////////////////////////////////////////////////////////////////////////////
            */
            $('.actual_material_name').click(function(){
				$('#actual_material_modal').modal('show');
                let string = $(this).children('input').val();
                if(string === '') {
                    $('#actual_material_name').val('');
                }else{
                    string = JSON.parse(string);
                    $('#actual_material_name').val(string.material_name);
                }
                $('#actual_material_model_title').html($(this).attr('id').split("actual_material_name-")[1]);
            });
            $('#actual_material_save').click(function(){
                param = {
                    model_name:$('#actual_material_model_title').html(),
                    material_name:$('#actual_material_name').val()
                };
				$.post( '/dashboard/set_actual_material_name', param, function( result ) {
                    if(result === 'success'){
                        $('#actual_material_name-' + param.model_name).children('input').val(JSON.stringify(param));
						// updateGraph(param.name,'start');
						// showMessage('success','SCHEDULE', "Saved Successfully.");
						$('#actual_material_modal').modal('hide');
					}
					else{
						// showMessage('error', 'SCHEDULE', "Failed");
					}
				});
            });
            /*
            //////////////////////////////////////////////////////////////////////////////////
            y axis value
            //////////////////////////////////////////////////////////////////////////////////
            */
            $('.y_axis_value').click(function(){
				$('#y_modal').modal('show');
                let string = $(this).children('input').val();
                if(string === '') {
                    $("#y_1").prop("checked", true);
                    $('#y_min').val('');
                    $('#y_max').val('');
                }else{
                    string = JSON.parse(string);
                    $("#y_"+string.option).prop("checked", true);
                    $('#y_min').val(string.min);
                    $('#y_max').val(string.max);
                }
                $('#y_model_title').html($(this).attr('id').split("y_axis_value-")[1]);
            });
            $('#y_save').click(function(){
                param = {
                    name:$('#y_model_title').html(),
                    option:$('input[name=y_style]:checked').val(),
                    min:$('#y_min').val(),
                    max:$('#y_max').val()
                };
				$.post( '/dashboard/set_y_axis', param, function( result ) {
                    if(result === 'success'){
                        $('#y_axis_value-' + param.name).children('input').val(JSON.stringify(param));
						updateGraph(param.name,'start');
						// showMessage('success','SCHEDULE', "Saved Successfully.");
						$('#y_modal').modal('hide');
					}
					else{
						// showMessage('error', 'SCHEDULE', "Failed");
					}

				});
            })
            $.toast({
                heading: 'Load Database',
                text: 'Load Latest Data',
                showHideTransition: 'slide',
                icon: 'success',
                hideAfter: 5000   // in milli seconds
            });
        </script>
